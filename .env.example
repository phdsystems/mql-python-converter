# Environment variables for Docker build and runtime
# Copy this file to .env and adjust if needed

# User and group IDs for the container user
# These should match your host user's UID/GID to avoid permission issues
USER_ID=1000
GROUP_ID=1000

# Container username (defaults to wineuser if not set)
CONTAINER_USER=wineuser

# VNC settings
VNC_PASSWORD=mt4vnc
VNC_RESOLUTION=1280x720
VNC_DEPTH=24